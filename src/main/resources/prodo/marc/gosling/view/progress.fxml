<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane style="-fx-background-color: #777777;" stylesheets="@css/song.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prodo.marc.gosling.controllers.ProgressController">
   <VBox alignment="CENTER" spacing="5.0">
   <HBox alignment="CENTER" spacing="10.0">
      <Label fx:id="progressLabel" text="xxx/yyy">
            <HBox.margin>
               <Insets left="5.0" />
            </HBox.margin></Label>
      <ProgressBar fx:id="progressBar" prefHeight="20.0" prefWidth="360.0" progress="0.0">
            <HBox.margin>
               <Insets right="5.0" top="5.0" />
            </HBox.margin></ProgressBar>
   </HBox>
      <Label fx:id="statusLabel" text="Please wait, importing files">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin></Label>
   </VBox>
</AnchorPane>
