<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prodo.marc.gosling.controllers.SongController">
    <TableView layoutX="1.0" layoutY="61.0" prefHeight="338.0" prefWidth="598.0">
      <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
      </columns>
    </TableView>
    <HBox layoutX="1.0" layoutY="8.0" prefHeight="50.0" spacing="20.0">
        <padding>
            <Insets left="10.0" top="10.0"/>
        </padding>
        <Button fx:id="songBackButton" mnemonicParsing="false" onAction="#backToMain" text="Back to main window"/>
        <Button fx:id="addSongButton" mnemonicParsing="false" onAction="#addSong2DB" text="Add new song"/>
    </HBox>
</AnchorPane>
